<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingTop="70dp"
    android:background="#ffffff"
    tools:context="com.example.khseob0715.sanfirst.navi_fragment.Fragment_AQIHistory">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_marginLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="80"
                    android:textColor="#5e5e5e"
                    android:textSize="80dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/AQItext"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="AQI"
                        android:textColor="#5e5e5e"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/AQItext2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="20dp"
                        android:text="/500"
                        android:textColor="#5e5e5e"
                        android:textSize="10dp" />
                </LinearLayout>

            </LinearLayout>

            <Button
                android:layout_width="160dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:background="@drawable/rounded_corner_button"
                android:text="Good"
                android:textColor="#ffffff"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/Temperature_Index"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="11°C"
                android:textColor="#5e5e5e"
                android:textSize="40dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="320dp"
            android:background="@drawable/border"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/callendar" />

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="TextView" />
            </LinearLayout>

            <ListView
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </LinearLayout>


    </LinearLayout>


    <TextView
        android:id="@+id/PollutantsIndex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_gravity="center"
        android:text="POLLUTANTS"
        android:textColor="#5e5e5e" />



    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_centerInParent="true"
        android:background="#aeaeae" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/pm25_text"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:text="PM2.5"
                        android:textColor="#000000"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/index_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="㎍/㎥"
                        android:textColor="#5e5e5e" />
                </LinearLayout>

                <TextView
                    android:id="@+id/pm25_data"
                    android:layout_width="270dp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="60"
                    android:textColor="#5e5e5e"
                    android:textSize="40dp" />
            </LinearLayout>

            <ProgressBar
                android:id="@+id/PM25_progress"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="170dp"
                android:layout_marginRight="10dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0"

                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/NO2_text"
                            android:layout_width="60dp"
                            android:layout_height="wrap_content"
                            android:text="NO2"
                            android:textColor="#000000"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/index_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="㎍/㎥"
                            android:textColor="#5e5e5e" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/no2_data"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="60dp"
                        android:layout_weight="0"
                        android:gravity="right"
                        android:text="60"
                        android:textColor="#5e5e5e"
                        android:textSize="40dp" />
                </LinearLayout>

                <ProgressBar
                    android:id="@+id/NO2_progress"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="190dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/co_text"
                            android:layout_width="60dp"
                            android:layout_height="wrap_content"
                            android:text="CO"
                            android:textColor="#000000"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/index_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="㎍/㎥"
                            android:textColor="#5e5e5e" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/co_data"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="45dp"
                        android:layout_weight="0"
                        android:gravity="right"
                        android:text="60"
                        android:textColor="#5e5e5e"
                        android:textSize="40dp" />
                </LinearLayout>

                <ProgressBar
                    android:id="@+id/CO_progress"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="170dp"
                android:layout_marginRight="10dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0"

                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/SO2_text"
                            android:layout_width="60dp"
                            android:layout_height="wrap_content"
                            android:text="NO2"
                            android:textColor="#000000"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/index_4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="㎍/㎥"
                            android:textColor="#5e5e5e" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/so2_data"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginLeft="60dp"
                        android:layout_weight="0"
                        android:gravity="right"
                        android:text="60"
                        android:textColor="#5e5e5e"
                        android:textSize="40dp" />
                </LinearLayout>

                <ProgressBar
                    android:id="@+id/SO2_progress"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="190dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/o3_text"
                            android:layout_width="60dp"
                            android:layout_height="wrap_content"
                            android:text="CO"
                            android:textColor="#000000"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/index_5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="㎍/㎥"
                            android:textColor="#5e5e5e" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/o3_data"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="45dp"
                        android:layout_weight="0"
                        android:gravity="right"
                        android:text="60"
                        android:textColor="#5e5e5e"
                        android:textSize="40dp" />
                </LinearLayout>

                <ProgressBar
                    android:id="@+id/O3_progress"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>


</LinearLayout>
